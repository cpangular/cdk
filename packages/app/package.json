{
  "name": "@cpangular/app",
  "version": "0.0.6",
  "scripts": {
    "build": "run-s build:*",
    "build:ng": "ng build app",
    "build:schematics": "tsc -p tsconfig.schematics.json",
    "postbuild": "copyfiles schematics/*/schema.json schematics/*/files/** schematics/collection.json ../../dist/app/",
    "watch": "ng build app --watch --configuration=development",
    "build-dev": "ng build app --configuration development",
    "package": "cd ../../dist/app && yarn pack --filename package.tgz",
    "release": "yarn publish ../../dist/app/package.tgz"
  },
  "schematics": "./schematics/collection.json",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": "https://github.com/cpangular/cdk",
  "peerDependencies": {
    "@angular/common": "~13.2.0",
    "@angular/core": "~13.2.0",
    "@angular/material": "~13.2.3",
    "@cpangular/material-dynamic-theming": "0.0.6",
    "@cpangular/cdk": "0.0.6",
    "@ngxs/store": "^3.7.3"
  },
  "dependencies": {
    "tslib": "^2.3.0"
  }
}