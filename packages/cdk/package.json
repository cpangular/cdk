  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": "https://github.com/cpangular/cdk",
  "peerDependencies": {
    "@angular/common": "^13.0.0",
    "@angular/core": "^13.0.0",
    "@angular/cdk": "^13.0.0",
    "linq": "^4.0.0"
  },
  "dependencies": {
    "tslib": "^2.3.0"
  }
}
