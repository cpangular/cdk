{
  "name": "cdk",
  "version": "17.1.0-dev.5",
  "scripts": {
    "prepare": "run-s prepare:*",
    "prepare:husky": "husky install",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier --write --ignore-unknown .",
    "fix:remove-git-tags": "pnpm run remove-git-tags",
    "remove-git-tags": "git tag | xargs git tag -d",
    "pre-commit": "run-s pre-commit:*",
    "pre-commit:remove-git-tags": "pnpm run remove-git-tags",
    "reset-hard": "git clean -dfx && git reset --hard && pnpm i --force",
    "build": "pnpm --recursive --stream --workspace-concurrency=1 run build",
    "watch": "pnpm --recursive --parallel --stream run watch",
    "pack": "pnpm --recursive --parallel --stream run pack",
    "release": "pnpm publish ./dist/pack/*.tgz --access public --no-git-checks",
    "set-version": "run-p \"set-version:* {1}\" --",
    "set-version:set-version": "pnpm version --allow-same-version --commit-hooks=false --git-tag-version=false",
    "set-version:set-project-versions": "pnpm --recursive --parallel --stream run set-version",
    "set-version-placeholders": "pnpm --recursive --parallel --stream run set-version 0.0.0-PLACEHOLDER",
    "actions-build": "pnpm run build",
    "actions-build-libs": "pnpm run build",
    "actions-pack-libs": "pnpm run pack",
    "actions-release-libs": "pnpm run release",
    "actions-set-version": "pnpm run set-version"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.1.0",
    "@angular/common": "^17.1.0",
    "@angular/compiler": "^17.1.0",
    "@angular/core": "^17.1.0",
    "@angular/forms": "^17.1.0",
    "@angular/platform-browser": "^17.1.0",
    "@angular/platform-browser-dynamic": "^17.1.0",
    "@angular/router": "^17.1.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.1.3",
    "@angular/cli": "^17.1.1",
    "@angular/compiler-cli": "^17.1.0",
    "@types/jasmine": "~5.1.0",
    "husky": "^9.0.10",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^15.2.2",
    "ng-packagr": "^17.1.0",
    "npm-run-all2": "^6.1.2",
    "prettier": "^3.2.5",
    "rxjs": "~7.8.1",
    "typescript": "~5.3.2"
  },
  "lint-staged": {
    "*": "prettier --write --ignore-unknown"
  }
}
