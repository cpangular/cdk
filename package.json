{
  "name": "@cpangular/cdk",
  "version": "0.0.4",
  "workspaces": [
    "./packages/*"
  ],
  "scripts": {
    "package": "ts-node .scripts/run.ts -P package",
    "release": "ts-node .scripts/run.ts -P release",
    "build": "ts-node .scripts/run.ts -p build",
    "build-dev": "ts-node .scripts/run.ts -p build-dev",
    "watch": "yarn run build-dev && ts-node .scripts/run.ts -P watch"
  },
  "private": true,
  "repository": "https://github.com/cpangular/cdk",
  "dependencies": {
    "@angular/animations": "~13.2.0",
    "@angular/cdk": "~13.2.3",
    "@angular/common": "~13.2.0",
    "@angular/compiler": "~13.2.0",
    "@angular/core": "~13.2.0",
    "@angular/forms": "~13.2.0",
    "@angular/material": "~13.2.3",
    "@angular/platform-browser": "~13.2.0",
    "@angular/platform-browser-dynamic": "~13.2.0",
    "@angular/router": "~13.2.0",
    "change-case": "^4.1.2",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.4",
    "@angular/cli": "~13.2.4",
    "@angular/compiler-cli": "~13.2.0",
    "@ngxs/store": "^3.7.3",
    "@types/node": "^12.11.1",
    "arg": "^5.0.1",
    "concurrently": "^7.0.0",
    "copyfiles": "^2.4.1",
    "dependency-graph": "^0.11.0",
    "lerna": "4",
    "linq": "^4.0.0",
    "ng-packagr": "^13.0.0",
    "npm-run-all2": "^5.0.2",
    "ts-node": "^10.5.0",
    "typescript": "~4.5.2",
    "yarn-workspaces-foreach": "^0.2.0"
  }
}
